import java.util.Scanner;

public class ContagemNumeros {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int numerosAte100 = 0;
        int numeros101a200 = 0;
        int numerosAcima200 = 0;
        
        // Solicita ao usuário que insira 20 números
        for (int i = 0; i < 20; i++) {
            System.out.println("Digite o número " + (i + 1) + ":");
            int numero = scanner.nextInt();
            
            // Verifica em qual intervalo o número se encontra e incrementa o contador apropriado
            if (numero >= 0 && numero <= 100) {
                numerosAte100++;
            } else if (numero >= 101 && numero <= 200) {
                numeros101a200++;
            } else {
                numerosAcima200++;
            }
        }
        
        // Exibe a contagem de números em cada intervalo
        System.out.println("Quantidade de números entre 0 e 100: " + numerosAte100);
        System.out.println("Quantidade de números entre 101 e 200: " + numeros101a200);
        System.out.println("Quantidade de números maiores que 200: " + numerosAcima200);
        
        scanner.close();
    }
}
