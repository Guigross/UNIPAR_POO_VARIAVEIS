import java.util.Scanner;

class HelloWorld {
    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);
        
    
        double Celsius = 0.0;
        System.out.println("Informe o valor para Fahrenheit");  
        
        Celsius = scanner.nextDouble();
    
        System.out.print ("Qual a Fahrenheit");
        
        
        double valorFahrenheit = (Celsius * 9/5) + 32;
        
        
        System.out.println("O valor Fahrenheit eh " + valorFahrenheit);
    
        scanner.close();
    }
}
